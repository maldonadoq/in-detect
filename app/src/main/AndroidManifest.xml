<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="maldonado.indetect">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_logo_neo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_logo_neo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".local.RealTimeActivity"></activity>
        <activity android:name=".server.user.ProfileActivity" />
        <activity android:name=".server.user.SignUpActivity" />
        <activity android:name=".server.user.SignInActivity" />
        <activity
            android:name=".local.LocalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".server.ServerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>